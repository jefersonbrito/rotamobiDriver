<?xml version='1.0' encoding='utf-8'?>
<widget id="org.myapp.map" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RotaMobi</name>
    <description>
        Solite seu veículo com praticidade e segurança.
    </description>
    <author email="jeferson@jefersonbrito.com.br" href="http://www.rotamobi.com.br">
        RotaMobi team.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
      
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="22" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyBXZKaHiUKgvdZPshKExhk5YjTkrfyLzRU" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="AIzaSyBXZKaHiUKgvdZPshKExhk5YjTkrfyLzRU" />
</widget>
